@model List<VehicleAllViewModel>
@{
    ViewData["Title"] = "My Cars";
}

<h2 class="text-center">@ViewData["Title"]</h2>
<br />

@if (!Model.Any())
{
    <h2 class="text-center">You do not have any cars yet!</h2>
}
else
{
    <div class="row">
        @foreach (VehicleAllViewModel car in Model)
        {
            <partial name="_CarPartial" model="car" />
        }
    </div>
}